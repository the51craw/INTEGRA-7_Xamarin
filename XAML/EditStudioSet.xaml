                        <Slider Grid.Column="1" x:Name="slStudioSetPartEQLowGain" Minimum="-15" Maximum="15" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" ValueChanged="slStudioSetPartEQLowGain_ValueChanged" />
                    </Grid>
                    <Grid Grid.Row="3">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <TextBlock Grid.Column="0" x:Name="tbStudioSetPartEQMidFreq" Text="EQ mid freq" Padding="4,0,4,0" HorizontalAlignment="Stretch" VerticalAlignment="Center" Grid.ColumnSpan="2" Margin="0,6,110.2,7"/>
                        <ComboBox Grid.Column="1" x:Name="cbStudioSetPartEQMidFreq" Padding="4,0,4,0" HorizontalAlignment="Stretch" VerticalAlignment="Center" SelectionChanged="cbStudioSetPartEQMidFreq_SelectionChanged" >
                            <x:String>200</x:String>
                            <x:String>250</x:String>
                            <x:String>315</x:String>
                            <x:String>400</x:String>
                            <x:String>500</x:String>
                            <x:String>630</x:String>
                            <x:String>800</x:String>
                            <x:String>1000</x:String>
                            <x:String>1250</x:String>
                            <x:String>1600</x:String>
                            <x:String>2000</x:String>
                            <x:String>2500</x:String>
                            <x:String>3150</x:String>
                            <x:String>4000</x:String>
                            <x:String>5000</x:String>
                            <x:String>6300</x:String>
                            <x:String>8000</x:String>
                        </ComboBox>
                    </Grid>
                    <Grid Grid.Row="4" Margin="2,0,2,2" Padding="4,0,0,0" BorderBrush="Black" BorderThickness="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock Grid.Column="0" x:Name="tbStudioSetPartEQMidGain" Text="EQ mid gain" HorizontalAlignment="Stretch" VerticalAlignment="Center" />
                        <Slider Grid.Column="1" x:Name="slStudioSetPartEQMidGain" Minimum="-15" Maximum="15" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" ValueChanged="slStudioSetPartEQMidGain_ValueChanged" />
                    </Grid>
                    <Grid Grid.Row="5">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <TextBlock Grid.Column="0" x:Name="tbStudioSetPartEQMidQ" Text="EQ mid Q" Padding="4,0,4,0" HorizontalAlignment="Stretch" VerticalAlignment="Center"/>
                        <ComboBox Grid.Column="1" x:Name="cbStudioSetPartEQMidQ" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" SelectionChanged="cbStudioSetPartEQMidQ_SelectionChanged" >
                            <x:String>0.5</x:String>
                            <x:String>1.0</x:String>
                            <x:String>2.0</x:String>
                            <x:String>4.0</x:String>
                            <x:String>8.0</x:String>
                        </ComboBox>
                    </Grid>
                    <Grid Grid.Row="6">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <TextBlock Grid.Column="0" x:Name="tbStudioSetPartEQHighFreq" Text="EQ high freq" Padding="4,0,4,0" HorizontalAlignment="Stretch" VerticalAlignment="Center"/>
                        <ComboBox Grid.Column="1" x:Name="cbStudioSetPartEQHighFreq" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" SelectionChanged="cbStudioSetPartEQHighFreq_SelectionChanged" >
                            <x:String>2000 Hz</x:String>
                            <x:String>4000 Hz</x:String>
                            <x:String>8000 Hz</x:String>
                        </ComboBox>
                    </Grid>
                    <Grid Grid.Row="7">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <TextBlock Grid.Column="0" x:Name="tbStudioSetPartEQHighGain" Text="EQ high gain" Padding="4,0,4,0" HorizontalAlignment="Stretch" VerticalAlignment="Center"/>
                        <Slider Grid.Column="1" x:Name="slStudioSetPartEQHighGain" Minimum="-15" Maximum="15" ValueChanged="slStudioSetPartEQHighGain_ValueChanged" />
                    </Grid>
                </Grid>
                <Grid Grid.Row="4" x:Name="Buttons" Margin="2,0,2,2" BorderThickness="1" BorderBrush="Black" Visibility="Visible">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                    </Grid.RowDefinitions>
                    <Grid Grid.Row="0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="5*" />
                            <ColumnDefinition Width="3*" />
                            <ColumnDefinition Width="8*" />
