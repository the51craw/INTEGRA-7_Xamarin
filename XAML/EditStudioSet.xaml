                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="6*" />
                    </Grid.RowDefinitions>
                    <Grid Grid.Row="0" Margin="2,2,2,2" Padding="4,0,0,0" BorderBrush="Black" BorderThickness="1">
                        <CheckBox x:Name="cbStudioSetPartEQSwitch" Grid.Column="1" Padding="4,5,0,0" Content="Equalizer on/off" HorizontalAlignment="Stretch" VerticalAlignment="Center" Click="cbStudioSetPartEQ_Click" />
                    </Grid>
                    <Grid Grid.Row="1" Margin="2,0,2,2" Padding="4,0,0,0" BorderBrush="Black" BorderThickness="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition />
                            <ColumnDefinition />
                        </Grid.ColumnDefinitions>
                        <TextBlock Grid.Column="0" Text="EQ low freq" HorizontalAlignment="Stretch" VerticalAlignment="Center" />
