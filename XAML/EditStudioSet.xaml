                            <x:String>200 Hz</x:String>
                            <x:String>400 Hz</x:String>
                        </ComboBox>
                    </Grid>
                    <Grid Grid.Row="2" Margin="2,0,2,2" Padding="4,0,0,0" BorderBrush="Black" BorderThickness="1">
