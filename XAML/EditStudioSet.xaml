                        <Button Grid.Column="2" x:Name="btnStudioSetSave" Content="Save" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Click="btnStudioSetSave_Click" />
                        <Button Grid.Column="3" x:Name="btnStudioSetDelete" Content="Delete" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Click="btnStudioSetDelete_Click" />
                        <Button Grid.Column="4" x:Name="btnStudioSetReturn" Content="Return" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Click="btnStudioSetReturn_Click" Grid.ColumnSpan="3" Margin="0,0,0.333,0" />
                    </Grid>
                </Grid>
            </Grid>
        </Grid>
    </Grid>
