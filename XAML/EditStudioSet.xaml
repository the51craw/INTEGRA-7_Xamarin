                    <Grid Grid.Row="3" Margin="2,0,2,2" Padding="4,0,0,0" BorderBrush="Black" BorderThickness="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock Grid.Column="0" x:Name="tbStudioSetPartMotionalSurroundAmbienceSendLevel" Text="Ambience send level" HorizontalAlignment="Stretch" VerticalAlignment="Center" />
                        <Slider Grid.Column="1" x:Name="slStudioSetPartMotionalSurroundAmbienceSendLevel" Minimum="0" Maximum="127" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" ValueChanged="slStudioSetPartMotionalSurroundAmbienceSendLevel_ValueChanged" />
                    </Grid>
                </Grid>

