                        <ComboBox Grid.Column="0" x:Name="cbStudioSetSlot" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" SelectionChanged="cbStudioSetSlot_SelectionChanged"  />
                        <TextBlock Grid.Column="1" Text="Name:" Margin="2,0,2,2" Padding="4,0,0,0" HorizontalAlignment="Stretch" VerticalAlignment="Center" />
                        <TextBox Grid.Column="2" x:Name="tbStudioSetName" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" VerticalContentAlignment="Center" KeyUp="tbStudioSetName_KeyUp" />
                    </Grid>
                    <Grid Grid.Row="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="3*" />
