                <Grid Grid.Row="3" x:Name="StudioSetPartEQ" Margin="2,0,2,2" BorderThickness="1" BorderBrush="Black" Visibility="Collapsed" >
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*" />
